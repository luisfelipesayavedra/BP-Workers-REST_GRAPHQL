generator client {
    provider = "prisma-client-js"
    output   = "../generated/client"
}

datasource db {
    provider = "sqlite"
    url      = "file:./db.db"
}

model Usuario {
    id         Int      @id @default(autoincrement())
    first_name String
    last_name  String
    phone      String
    email      String
    password   String
    imagen     String
    createdAt  DateTime @default(now())
    updatedAt  DateTime @updatedAt
}

model Bulkings {
    id            Int             @id @default(autoincrement())
    percentage    Float
    createdAt     DateTime        @default(now())
    updatedAt     DateTime        @updatedAt
    BulkingsError BulkingsError[]
}

model BulkingsError {
    id          Int      @id @default(autoincrement())
    error       String
    BuilkingsId Int
    bulking     Bulkings @relation(fields: [BuilkingsId], references: [id])
}
